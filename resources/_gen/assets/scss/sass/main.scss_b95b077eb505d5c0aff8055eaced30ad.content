@font-face{font-family:envy code r;src:url("../font/Envy Code R.ttf")format("truetype");font-weight:400;font-size:10pt;font-style:normal}body{color:#bfbfbf;background-color:#121212;font-family:arial,helvetica,sans-serif}h1,h2,h3,h4,h5,h6{font-family:envy code r,monospace;font-size:14pt}.grey{color:gray}.light-orange{color:#d1b38a}a:link,a:visited,a:active{color:#e1aa5d;text-decoration:none}a:hover{color:#ff9600;text-decoration:none}.caption{font-family:envy code r,monospace}nav{background-color:#333;font-family:envy code r,monospace;height:26px;font-size:10pt;padding-bottom:10px}.navbar .nav-item .nav-link{padding:auto;text-align:center;background-color:#1f1f1f}a.nav-link{text-align:center;margin:auto;padding:0;background-color:#1f1f1f}li.navbar-item.active a{color:#ff9600;text-decoration:none}div.item{background-color:#121212;color:#bfbfbf;border:5px solid #1f1f1f;border-radius:0;margin-top:5px;padding:0;font-size:10pt}div.header{font-family:envy code r,monospace;background-color:#171717;border-bottom:1px solid #1f1f1f;margin:0;padding:0}.header h1{margin-bottom:0;font-weight:400;font-size:10pt}p{padding-left:5px;padding-right:5px}img{border:3px solid #262626;max-width:98%;margin:4px;display:block}#index h1,h2{color:#d1b38a;font-size:15pt}